<?xml version="1.0" encoding="UTF-8"?>
<Defs>
   <!-- Gas turbine -->
   <ThingDef ParentName="BuildingBase">
      <defName>CA_GasTurbine</defName>
      <label>gas-powered turbine</label>
      <description>A helixien gas powered turbojet. Although cumbersome, it produces a lot power and can be linked to the helixien gas network.</description>
      <thingClass>Building</thingClass>
      <drawerType>MapMeshAndRealTime</drawerType>
      <graphicData>
         <texPath>Things/Buildings/Power/HelixienTurbine/HelixienTurbine</texPath>
         <graphicClass>Graphic_Single</graphicClass>
         <drawSize>(4,4)</drawSize>
         <shadowData>
            <volume>(1.93,1,1.69)</volume>
            <offset>(-0.03,0,-0.1)</offset>
         </shadowData>
      </graphicData>
      <altitudeLayer>Building</altitudeLayer>
      <passability>PassThroughOnly</passability>
      <pathCost>50</pathCost>
      <blockWind>true</blockWind>
      <fillPercent>1.0</fillPercent>
      <castEdgeShadows>false</castEdgeShadows>
      <rotatable>false</rotatable>
      <statBases>
         <MaxHitPoints>300</MaxHitPoints>
         <WorkToBuild>2500</WorkToBuild>
         <Flammability>1.0</Flammability>
         <Beauty>-20</Beauty>
      </statBases>
      <tickerType>Normal</tickerType>
      <canOverlapZones>false</canOverlapZones>
      <size>(4,4)</size>
      <building>
         <destroySound>BuildingDestroyed_Metal_Medium</destroySound>
      </building>
      <costList>
         <Steel>220</Steel>
         <ComponentIndustrial>8</ComponentIndustrial>
      </costList>
      <comps>
			<li Class="GasNetwork.CompProperties_GasTrader">
				<compClass>GasNetwork.CompGasTrader</compClass>
				<gasConsumption>150</gasConsumption>
			</li>
			<li Class="CompProperties_Power">
				<transmitsPower>true</transmitsPower>
				<compClass>GasNetwork.CompGasPowerPlant</compClass>
				<basePowerConsumption>-6000</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable">
				<commandTexture>Things/Special/Fluid/VPE_OverlayBase</commandTexture>
				<commandLabelKey>Fluffy.GasNetwork.CommandToggleGas.Label</commandLabelKey>
				<commandDescKey>Fluffy.GasNetwork.CommandToggleGas.Desc</commandDescKey>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>6</glowRadius>
				<glowColor>(90,140,90,0)</glowColor>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>GasNetwork.CompGasHeatPusher</compClass>
				<heatPerSecond>8</heatPerSecond>
			</li>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="CompProperties_Explosive">
				<explosiveRadius>0</explosiveRadius>
				<explosiveExpandPerFuel>0.95</explosiveExpandPerFuel>
				<explosiveDamageType>Flame</explosiveDamageType>
				<destroyThingOnExplosionSize>2</destroyThingOnExplosionSize>
				<startWickHitPointsPercent>0.333</startWickHitPointsPercent>
				<preExplosionSpawnThingDef>Filth_Fuel</preExplosionSpawnThingDef>
				<preExplosionSpawnChance>1</preExplosionSpawnChance>
				<wickTicks>70~150</wickTicks>
			</li>
		</comps>
      <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
      <designationCategory>Power</designationCategory>
      <designationHotKey>Misc3</designationHotKey>
      <constructEffect>ConstructMetal</constructEffect>
      <researchPrerequisites>
         <li>Electricity</li>
      </researchPrerequisites>
      <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
   </ThingDef>

   <!-- Infinite chemfuel pump -->
   <ThingDef ParentName="BuildingBase">
      <defName>CE_InfiniteChemfuelPump</defName>
      <label>infinite chemfuel pump</label>
      <description>A large pump capable of generating chemfuel, using atoms extracted from the air and crust. Needs to be placed on the chemfuel pond but wont spend the ponds capacity.</description>
      <thingClass>Building</thingClass>
      <graphicData>
         <texPath>Things/Buildings/Special/ChemfuelExtractor/ChemfuelExtractor</texPath>
         <graphicClass>Graphic_Single</graphicClass>
         <drawSize>(4,4)</drawSize>
      </graphicData>
      <altitudeLayer>Building</altitudeLayer>
      <passability>PassThroughOnly</passability>
      <pathCost>150</pathCost>
      <blockWind>true</blockWind>
      <fillPercent>1.0</fillPercent>
      <castEdgeShadows>false</castEdgeShadows>
      <rotatable>false</rotatable>
      <statBases>
         <MaxHitPoints>750</MaxHitPoints>
         <WorkToBuild>2500</WorkToBuild>
         <Flammability>1.5</Flammability>
         <Beauty>-20</Beauty>
      </statBases>
      <tickerType>Normal</tickerType>
      <canOverlapZones>false</canOverlapZones>
      <size>(4,4)</size>
      <costList>
         <Steel>300</Steel>
         <ComponentIndustrial>8</ComponentIndustrial>
      </costList>
      <placeWorkers>
         <li>VanillaPowerExpanded.PlaceWorker_OnChemfuelPond</li>
      </placeWorkers>
      <comps>
         <li Class="CompProperties_Power">
            <compClass>CompPowerTrader</compClass>
            <basePowerConsumption>500</basePowerConsumption>
         </li>
			<li Class="VanillaPowerExpanded.CompProperties_ChemfuelPump">
				<compClass>VFEPowerAppliances.CompChemfuelPumpInfinite</compClass>
				<fuelProduced>65</fuelProduced>
				<fuelInterval>6</fuelInterval>
			</li>
         <li Class="CompProperties_Glower">
            <glowRadius>6</glowRadius>
            <glowColor>(166, 20, 20,0)</glowColor>
         </li>
         <li Class="CompProperties_Breakdownable" />
         <li Class="CompProperties_Explosive">
            <explosiveRadius>0</explosiveRadius>
            <explosiveExpandPerFuel>0.95</explosiveExpandPerFuel>
            <explosiveDamageType>Flame</explosiveDamageType>
            <destroyThingOnExplosionSize>2</destroyThingOnExplosionSize>
            <startWickHitPointsPercent>0.333</startWickHitPointsPercent>
            <preExplosionSpawnThingDef>Filth_Fuel</preExplosionSpawnThingDef>
            <preExplosionSpawnChance>1</preExplosionSpawnChance>
            <wickTicks>70~150</wickTicks>
         </li>
		 <li Class="PipeSystem.CompProperties_ResourceStorage">
        <pipeNet>VCHE_ChemfuelNet</pipeNet>
        <storageCapacity>150</storageCapacity>
        <barSize>(1.3, 0.25)</barSize>
        <margin>0.1</margin>
        <centerOffset>(0, 0, 0.35)</centerOffset>
        <extractOptions>
          <texPath>UI/Gizmos/ExtractChemfuel</texPath>
          <labelKey>VCHE_ExtractChemfuel</labelKey>
          <descKey>VCHE_ExtractChemfuelDesc</descKey>
          <extractAmount>150</extractAmount>
          <thing>Chemfuel</thing>
          <ratio>1</ratio>
        </extractOptions>
      </li>
      </comps>
      <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
      <designationCategory>Power</designationCategory>
      <designationHotKey>Misc3</designationHotKey>
      <constructEffect>ConstructMetal</constructEffect>
      <researchPrerequisites>
         <li>BiofuelRefining</li>
      </researchPrerequisites>
      <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
   </ThingDef>
<!-- Chemfuel Drill -->
<ThingDef ParentName="BuildingBase">
		<defName>SCP_ChemDrill</defName>
		<label>gas powered excavator</label>
		<description>A deep-crust drilling excavator for extracting minerals from deep under the surface. Use a ground-penetrating scanner to detect the resources. This drill will yield stone chunks if no resources are present. Can be repositioned freely.</description>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>Things/Building/GasMachines/GasPoweredExcavator</texPath>
			<drawSize>(1,1)</drawSize>
			<drawRotated>false</drawRotated>
			<shadowData>
				<volume>(0.52, 0.53, 0.09)</volume>
				<offset>(0,0,-0.43)</offset>
			</shadowData>
		</graphicData>
		<size>(1,1)</size>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<fillPercent>0.5</fillPercent>
		<hasInteractionCell>true</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<designationCategory>Production</designationCategory>
		<designationHotKey>Misc12</designationHotKey>
		<specialDisplayRadius>2.6</specialDisplayRadius>
		<rotatable>true</rotatable>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
		<uiIconOffset>(0, 0.03)</uiIconOffset>
		<uiIconScale>0.85</uiIconScale>
		<minifiedDef>MinifiedThing</minifiedDef>
		<building>
			<uninstallWork>1800</uninstallWork>
		</building>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<costList>
			<Steel>100</Steel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<placeWorkers>
			<li>PlaceWorker_DeepDrill</li>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<researchPrerequisites>
			<li>DeepDrilling</li>
		</researchPrerequisites>
		<statBases>
			<MaxHitPoints>450</MaxHitPoints>
			<WorkToBuild>10000</WorkToBuild>
			<Flammability>0.75</Flammability>
			<Beauty>-25</Beauty>
			<Mass>35</Mass>
		</statBases>
		<comps>
		  <li Class="CompProperties_Refuelable">
                <fuelConsumptionRate>10.0</fuelConsumptionRate>
                <fuelCapacity>50.0</fuelCapacity>
                <fuelFilter>
                    <thingDefs>
                        <li>Chemfuel</li>
                    </thingDefs>
                </fuelFilter>
                <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
            </li>
            <li Class="CompProperties_Flickable" />
            <li Class="CompProperties_Glower">
                <glowRadius>6</glowRadius>
                <glowColor>(217,112,33,0)</glowColor>
            </li>
            <li Class="CompProperties_HeatPusher">
                <compClass>CompHeatPusherPowered</compClass>
                <heatPerSecond>12</heatPerSecond>
            </li>
			<li Class="CompProperties_DeepDrill"/>
			<li Class="CompProperties_CreatesInfestations"/>
		</comps>
	</ThingDef>
</Defs>