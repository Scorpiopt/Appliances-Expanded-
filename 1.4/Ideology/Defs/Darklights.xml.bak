 <Defs>


	<!-- Chemfuel sunlamp -->
	<ThingDef ParentName="LampBase">
		<defName>CA_SunlampDark</defName>
		<label>Darklight chemfuel sun lamp</label>
		<description>An industrial lamp which lights an area  with Darklight enough to grow crops, but consumes a huge amount of chemfuel. It also has a slightly lower lighting radius. Automatically turns itself off at night.</description>
		<thingClass>Building_SunLamp</thingClass>
		<graphicData>
			<texPath>Things/Buildings/Production/ChemfuelSunLampDark/ChemfuelSunLampDark</texPath><!-- Replace texture here -->
		</graphicData>
		<blockWind>true</blockWind>
		<tickerType>Normal</tickerType>
		<statBases>
			<WorkToBuild>330</WorkToBuild>
			<Mass>4.5</Mass>
		</statBases>
		<costList>
			<Steel>40</Steel>
		</costList>
		<comps>
			<li Class="CompProperties_Glower">
				<overlightRadius>10.0</overlightRadius>
				<glowRadius>20</glowRadius>
				<glowColor>(78, 226, 229, 0)</glowColor>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>18.0</fuelConsumptionRate>
				<fuelCapacity>90.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>Chemfuel</li>
					</thingDefs>
				</fuelFilter>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
			</li>
			<li Class="CompProperties_Schedule">
				<startTime>0.25</startTime>
				<endTime>0.8</endTime>
				<offMessage>Off for plant resting period</offMessage>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>3</heatPerSecond>
			</li>
		</comps>
		<specialDisplayRadius>5.8</specialDisplayRadius>
         <researchPrerequisites>
			<li>Electricity</li>
		</researchPrerequisites>
	</ThingDef>

	<!-- Chemfuel standing lamp -->
	<ThingDef ParentName="StandingLampBase">
		<defName>CA_ChemfuelStandingLampDArk</defName>
		<label>Darklightchemfuel-powered  standing  lamp</label>
		<description>A chemfuel standing lamp that lights an area. People need light to move and work at full speed.</description>
		<designationHotKey>Misc11</designationHotKey>
		<graphicData>
			<texPath>Things/Buildings/Production/ChemfuelExternalLampDark/ChemfuelExternalLampDark</texPath>
		</graphicData>
		<statBases>
			<WorkToBuild>100</WorkToBuild>
			<Mass>4.5</Mass>
		</statBases>
		<costList>
			<Steel>20</Steel>
		</costList>
		<comps Inherit="false">
			<li Class="CompProperties_Glower">
				<glowRadius>12</glowRadius>
				<glowColor>(78, 226, 229, 0)</glowColor>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>0.5</fuelConsumptionRate>
				<fuelCapacity>10.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>Chemfuel</li>
					</thingDefs>
				</fuelFilter>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
			</li>
			<li Class="CompProperties_Styleable"/>
		</comps>
		<tickerType>Normal</tickerType>
           <researchPrerequisites>
			<li>Electricity</li>
		</researchPrerequisites>
	</ThingDef>

	
	<!-- Chemfuel floodlight -->
	<ThingDef ParentName="StandingLampBase">
		<defName>CA_ChemfuelFloodlightDark</defName>
		<label>Darklight chemfuel floodlight</label>
		<description>A tall chemfuel-powered lamp that lights an area, able to be placed outside without catching fire, making it ideal for road lighting and only turns on at night.</description>
		<designationHotKey>Misc11</designationHotKey>
		<graphicData>
			<texPath>Things/Buildings/Production/ChemfuelFloodlightDark/ChemfuelFloodlightDark</texPath>
			<drawSize>(2,3.5)</drawSize>
		</graphicData>
		<costList>
			<Steel>80</Steel>
		</costList>
		<comps Inherit="false">
			<li Class="CompProperties_Glower">
				<glowRadius>18</glowRadius>
				<glowColor>(78, 226, 229, 0)</glowColor>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>2.0</fuelConsumptionRate>
				<fuelCapacity>10.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>Chemfuel</li>
					</thingDefs>
				</fuelFilter>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
			</li>
			<li Class="CompProperties_Styleable"/>
		</comps>
		<tickerType>Normal</tickerType>
       <researchPrerequisites>
			<li>Electricity</li>
		</researchPrerequisites>
	</ThingDef>


	<ThingDef ParentName="StandingLampBase">
		<defName>CAS_ChemfuelWallLightDArk</defName>
		<label>Darklight Chemfuel wall light</label>
		<graphicData>
			<texPath>Things/Buildings/ChemfuelWallLightDark/ChemfuelWallLightDark</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shadowData>
				<volume>(0, 0, 0)</volume>
				<offset>(0,0,0)</offset>
			</shadowData>
		</graphicData>
		<costList>
			<Steel>20</Steel>
		</costList>
		<altitudeLayer>Item</altitudeLayer>
		<description>A Chemfuel-powered wall light that lights an area. People need light to move and work at full speed. Can be safely placed outdoors.</description>
		<comps Inherit="false">
			<li Class="CompProperties_Glower">
				<glowRadius>12</glowRadius>
				<glowColor>(78, 226, 229, 0)</glowColor>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>0.5</fuelConsumptionRate>
				<fuelCapacity>10.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>Chemfuel</li>
					</thingDefs>
				</fuelFilter>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
			</li>
			<li Class="CompProperties_Flickable"/>
		</comps>
		<building>
			<isEdifice>false</isEdifice>
			<canPlaceOverWall>true</canPlaceOverWall>
		</building>
		<rotatable>true</rotatable>
		<clearBuildingArea>false</clearBuildingArea>
		<defaultPlacingRot>South</defaultPlacingRot>
		<tickerType>Normal</tickerType>
  <researchPrerequisites>
      <li>Electricity</li>
    </researchPrerequisites>
	</ThingDef>
	ThingDef MayRequire="Ludeon.RimWorld.Royalty" ParentName="FurnitureBase">
  
    <ThingDef Name="Brazier" ParentName="FurnitureBase">
        <defName>CA_ChemfuelBrazierDark</defName>
        <label>chemfuel brazier</label>
        <description>A Darklight Chemfuel Brazier. Will light the surroundings and create heat.</description>
        <category>Building</category>
        <tickerType>Normal</tickerType>
        <drawerType>RealtimeOnly</drawerType>
        <fillPercent>0.15</fillPercent>
        <statBases>
            <MaxHitPoints>80</MaxHitPoints>
            <WorkToBuild>100</WorkToBuild>
            <Flammability>0</Flammability>
            <Mass>15</Mass>
            <MeditationFocusStrength>0.0</MeditationFocusStrength>
        </statBases>
        <selectable>true</selectable>
        <designationCategory>Furniture</designationCategory>
        <uiIconOffset>(0, -0.04)</uiIconOffset>
        <minifiedDef>MinifiedThing</minifiedDef>
        <costList>
            <Steel>40</Steel>
            <ComponentIndustrial>1</ComponentIndustrial>
        </costList>
        <placeWorkers>
            <li>PlaceWorker_Heater</li>
            <li>PlaceWorker_MeditationOffsetBuildingsNear</li>
        </placeWorkers>
        <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
        <comps>
            <li Class="CompProperties_Refuelable">
                <fuelConsumptionRate>1.0</fuelConsumptionRate>
                <fuelCapacity>5.0</fuelCapacity>
                <fuelConsumptionPerTickInRain>0.0006</fuelConsumptionPerTickInRain>
                <fuelFilter>
                    <thingDefs>
                        <li>Chemfuel</li>
                    </thingDefs>
                </fuelFilter>
                <initialFuelPercent>0</initialFuelPercent>
                <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
            </li>
            <li Class="CompProperties_Glower">
                <glowRadius>10</glowRadius>
                <glowColor>(78, 226, 229, 0)</glowColor><!-- This is a specific darklight color, make sure it matches DarklightUtility.IsDarklight -->
            </li>
            <li Class="CompProperties_HeatPusher">
                <compClass>CompHeatPusherPowered</compClass>
                <heatPerSecond>3.5</heatPerSecond>
                <heatPushMaxTemperature>23</heatPushMaxTemperature>
            </li>
            <li Class="CompProperties_FireOverlay">
                <fireSize>1</fireSize>
                <offset>(0,0,0.18)</offset>
            </li>
            <li Class="CompProperties_MeditationFocus">
                <statDef>MeditationFocusStrength</statDef>
                <focusTypes>
                    <li>Flame</li>
                </focusTypes>
                <offsets>
                    <li Class="FocusStrengthOffset_Lit">
                        <offset>0.15</offset>
                    </li>
                    <li Class="FocusStrengthOffset_BuildingDefsLit">
                        <defs>
                            <li>Campfire</li>
                            <li>TorchLamp</li>
                            <li MayRequire="Ludeon.RimWorld.Royalty">Brazier</li>
                            <li MayRequire="Ludeon.RimWorld.Royalty,Ludeon.RimWorld.Ideology">DarklightBrazier</li>
                            <li MayRequire="Ludeon.RimWorld.Ideology">Darktorch</li>
                            <li MayRequire="Ludeon.RimWorld.Ideology">DarktorchFungus</li>
                        </defs>
                        <offsetPerBuilding>0.02</offsetPerBuilding>
                        <radius>9.9</radius>
                        <maxBuildings>8</maxBuildings>
                        <explanationKey>MeditationFocusPerFlame</explanationKey>
                        <explanationKeyAbstract>MeditationFocusPerFlameAbstract</explanationKeyAbstract>
                    </li>
                </offsets>
            </li>
        </comps>
        <canOverlapZones>false</canOverlapZones>
        <altitudeLayer>Building</altitudeLayer>
        <passability>PassThroughOnly</passability>
        <pathCost>42</pathCost>
        <rotatable>false</rotatable>
        <researchPrerequisites>
            <li>ComplexFurniture</li>
        </researchPrerequisites>
        <graphicData>
            <!-- art+to -->
            <texPath>Things/Buildings/Misc/ChemfuelBrazierDark/ChemfuelBrazier</texPath> <!-- Replace texture here -->
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <shadowData>
                <volume>(0.55, 0.35, 0.3)</volume>
                <offset>(0, 0, -0.28)</offset>
            </shadowData>
            <damageData>
                <rect>(0.2,0.3,0.6,0.6)</rect>
            </damageData>
        </graphicData>
    </ThingDef>
</Defs>