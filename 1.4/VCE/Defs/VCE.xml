<?xml version="1.0" encoding="UTF-8"?>
<Defs>
   <!-- Chemfuel deep frier -->
   <ThingDef ParentName="BuildingBase">
      <defName>CE_ChemfuelDeepFrier</defName>
      <label>chemfuel deep fryer</label>
      <description>A deep fryer is a kitchen appliance used for deep frying, and this particular model uses chemfuel to power itself. Deep frying is a method of cooking by submerging food into oil at high heat, typically between temperatures of 350 째F and 375 째F (175 째C to 190 째C).</description>
      <thingClass>Building_WorkTable</thingClass>
      <graphicData>
         <texPath>Things/Buildings/ChemfuelDeepFryer/ChemfuelDeepFryer</texPath>
         <graphicClass>Graphic_Multi</graphicClass>
         <drawSize>(1.5,1.5)</drawSize>
         <damageData>
            <cornerTL>Damage/Corner</cornerTL>
            <cornerTR>Damage/Corner</cornerTR>
            <cornerBL>Damage/Corner</cornerBL>
            <cornerBR>Damage/Corner</cornerBR>
         </damageData>
      </graphicData>
      <tickerType>Normal</tickerType>
      <costList>
         <Steel>75</Steel>
         <Chemfuel>20</Chemfuel>
      </costList>
      <altitudeLayer>Building</altitudeLayer>
      <rotatable>true</rotatable>
      <neverMultiSelect>true</neverMultiSelect>
      <uiIconScale>0.8</uiIconScale>
      <fillPercent>0.5</fillPercent>
      <useHitPoints>True</useHitPoints>
      <statBases>
         <Mass>10</Mass>
         <WorkToBuild>1500</WorkToBuild>
         <MaxHitPoints>150</MaxHitPoints>
         <Flammability>1.5</Flammability>
      </statBases>
      <inspectorTabs>
         <li>ITab_Bills</li>
      </inspectorTabs>
      <thingCategories>
         <li>BuildingsProduction</li>
      </thingCategories>
      <building>
         <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
         <sowTag>SupportPlantsOnly</sowTag>
         <canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
         <ai_chillDestination>false</ai_chillDestination>
         <artificialForMeditationPurposes>false</artificialForMeditationPurposes>
         <buildingTags>
            <li>Production</li>
         </buildingTags>
      </building>
      <size>(1,1)</size>
      <designationCategory>Production</designationCategory>
      <passability>PassThroughOnly</passability>
      <pathCost>70</pathCost>
      <hasInteractionCell>True</hasInteractionCell>
      <interactionCellOffset>(0,0,-1)</interactionCellOffset>
      <surfaceType>Item</surfaceType>
      <researchPrerequisites>
         <li>VCE_DeepFrying</li>
      </researchPrerequisites>
      <comps>
         <li Class="CompProperties_Refuelable">
            <fuelConsumptionRate>160.0</fuelConsumptionRate>
            <fuelCapacity>40</fuelCapacity>
            <fuelFilter>
               <thingDefs>
                  <li>Chemfuel</li>
               </thingDefs>
            </fuelFilter>
            <initialFuelPercent>1</initialFuelPercent>
            <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
            <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
         </li>
         <li Class="CompProperties_HeatPusher">
            <compClass>CompHeatPusherPowered</compClass>
            <heatPushMaxTemperature>36</heatPushMaxTemperature>
            <heatPerSecond>6</heatPerSecond>
         </li>
         <li Class="CompProperties_Flickable" />
         <li Class="CompProperties_AffectedByFacilities">
            <linkableFacilities>
               <li>ToolCabinet</li>
            </linkableFacilities>
         </li>
         <li>
            <compClass>CompReportWorkSpeed</compClass>
         </li>
      </comps>
      <placeWorkers>
         <li>PlaceWorker_ShowFacilitiesConnections</li>
         <li>PlaceWorker_ReportWorkSpeedPenalties</li>
         <li>PlaceWorker_PreventInteractionSpotOverlap</li>
      </placeWorkers>
      <minifiedDef>MinifiedThing</minifiedDef>
        <modExtensions>
            <li Class="RecipeInheritance.ThingDefExtension">
                <inheritRecipesFrom>
                    <li>VCE_DeepFrier</li>
                </inheritRecipesFrom>
            </li>
        </modExtensions>
   </ThingDef>
