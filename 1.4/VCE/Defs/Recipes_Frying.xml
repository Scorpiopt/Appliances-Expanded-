<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>CE_DeepFryMeatsNoBadHediff</defName>
		<label>Healthy fried meats x20</label>
		<description>Deep fry large meat chunks. Air fried food is very tasty.</description>
		<jobString>Deep frying meats.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>

		<recipeUsers>
			<li>CE_AirFryer</li>					
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>

					</categories>
				</filter>
				<count>1</count>
			</li>

		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>				
			</categories>	
			<disallowedThingDefs>

				<li>VCE_DeepFriedBigMeat</li>
				<li>VCE_DeepFriedFish</li>
			</disallowedThingDefs>			
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>				
			</categories>
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>Meat_Megaspider</li>
				<li>VCE_DeepFriedBigMeat</li>
				<li>CE_DeepFriedBigMeatNoBadHediff</li>
				<li>VCE_DeepFriedFish</li>
			</disallowedThingDefs>
			<disallowedCategories>	

			</disallowedCategories>
		</defaultIngredientFilter>
		<products>
			<CE_DeepFriedBigMeatNoBadHediff>4</CE_DeepFriedBigMeatNoBadHediff>
		</products>

	</RecipeDef>

	<RecipeDef>
		<defName>CE_DeepFryVegetables</defName>
		<label>healthy fried vegetables x20</label>
		<description>Healthy fried vegetables. Air fried food is very tasty.</description>
		<jobString>Deep frying vegetables.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>

		<recipeUsers>
			<li>CE_AirFryer</li>					
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>

					</categories>
				</filter>
				<count>1</count>
			</li>

		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>PlantFoodRaw</li>				
			</categories>		
			<disallowedThingDefs>
				<li>VCE_DeepFriedVegetables</li>

			</disallowedThingDefs>			
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>PlantFoodRaw</li>				
			</categories>
			<disallowedThingDefs>
				<li>VCE_DeepFriedVegetables</li>
				<li>CE_DeepFriedVegetablesNoBadHediff</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
		<products>
			<CE_DeepFriedVegetablesNoBadHediff>4</CE_DeepFriedVegetablesNoBadHediff>
		</products>

	</RecipeDef>







</Defs>
